     body(class="default_home")

          include ../partials/header.pug
          include ../partials/main-menu.pug  
          include ../partials/mini-cart.pug
          include ../partials/mobile-menu-area.pug

          block main

          include ../partials/footer.pug
          include ../partials/scroll-button.pug

          // jquery library js
          script(src="https://cdn.jsdelivr.net/npm/jquery@3.7.1/dist/jquery.min.js") 
          // bootstrap js
          script(src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/js/bootstrap.min.js")
          // font-awesome js
          script(src="/client/assets/js/Font-Awesome.js")
          // counter js
          script(src="https://cdn.jsdelivr.net/npm/jquery-countup@1.0.1/src/jquery.countUp.min.js")
          // nice select js
          script(src="https://cdn.jsdelivr.net/npm/jquery-nice-select@1.1.0/js/jquery.nice-select.min.js")
          // select 2 js
          script(src="https://cdn.jsdelivr.net/npm/select2@4.1.0-rc.0/dist/js/select2.min.js")
          // simply countdown js
          script(src="/client/assets/js/simplyCountdown.js")
          // slick slider js
          script(src="https://cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.min.js")
